local function URMC_fake_script() -- OpenAimlockFrame.LocalScript 
	local script = Instance.new('LocalScript', OpenAimlockFrame)

	local AimlockFrame = script.Parent.Parent.Aimlock
	local AnimationFrame = script.Parent.Parent.Animation
	local AutoBuyFrame = script.Parent.Parent["AutoBuy "]
	local AutoFarmFrame = script.Parent.Parent.AutoFarm
	local CreditsFrame = script.Parent.Parent.Credits
	local EspFrame = script.Parent.Parent.Esp
	local ExtraStuffFrame = script.Parent.Parent["Extra Stuff"]
	local FovFrame = script.Parent.Parent.Fov
	local FunFrame = script.Parent.Parent.Fun
	local PremiumFrame = script.Parent.Parent.Premium
	local SilentAimFrame = script.Parent.Parent["Silent Aim"]
	local TargetFrame = script.Parent.Parent.Target
	local TeleportFrame = script.Parent.Parent.Teleport
	
	script.Parent.MouseButton1Click:Connect(function()
		if AnimationFrame.Visible == true then
			AnimationFrame.Visible = false
		end
		if AutoBuyFrame.Visible == true then
			AutoBuyFrame.Visible = false
		end
		if AutoFarmFrame.Visible == true then
			AutoFarmFrame.Visible = false
		end
		if CreditsFrame.Visible == true then
			CreditsFrame.Visible = false
		end
		if EspFrame.Visible == true then
			EspFrame.Visible = false
		end
		if ExtraStuffFrame.Visible == true then
			ExtraStuffFrame.Visible = false
		end
		if FovFrame.Visible == true then
			FovFrame.Visible =  false
		end
		if FunFrame.Visible == true then
			FunFrame.Visible = false
		end
		if PremiumFrame.Visible == true then
			PremiumFrame.Visible = false
		end
		if SilentAimFrame.Visible == true then
			SilentAimFrame.Visible = false
		end
		if TargetFrame.Visible == true then
			TargetFrame.Visible = false
		end
		if TeleportFrame.Visible == true then
			TeleportFrame.Visible = false
		end
		if AimlockFrame.Visible == false then
			AimlockFrame.Visible = true
		end
	end)
end
coroutine.wrap(URMC_fake_script)()
